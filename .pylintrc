[MASTER]
load-plugins=pylint_django
django-settings-module=secretariaVirtual.settings
ignore=migrations,venv,.venv,CVS

[FORMAT]
max-line-length=120

[MESSAGES CONTROL]
disable=
    missing-docstring,
    invalid-name,
    too-few-public-methods,
    too-many-arguments,
    protected-access,
    duplicate-code

[DESIGN]
max-complexity=25
max-attributes=15
max-locals=20

